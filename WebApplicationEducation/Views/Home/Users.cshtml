@{
    ViewData["Title"] = "Users";
}
<h1>Список пользователей:</h1>
<hr />

@foreach (var item in Model)
{
    <div>
        <div>Id: @item.Id</div>
        <div>Name: @item.Name</div>
        <div>Balance: @item.Balance</div>
        <div>Created date: @item.Created</div>
    </div>
    <hr />
}
